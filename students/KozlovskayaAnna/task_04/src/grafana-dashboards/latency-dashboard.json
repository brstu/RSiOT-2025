{
  "dashboard": {
    "title": "Request Latency - Variant 8",
    "tags": ["monitoring", "latency", "variant-8", "as63-220012-v8"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "P95 Latency (SLO: 350ms)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(app08_http_request_duration_seconds_bucket[5m])) by (le)) * 1000",
            "legendFormat": "P95",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 300, "color": "yellow"},
                {"value": 350, "color": "red"}
              ]
            },
            "decimals": 2
          }
        }
      },
      {
        "id": 2,
        "title": "P99 Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(app08_http_request_duration_seconds_bucket[5m])) by (le)) * 1000",
            "legendFormat": "P99",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
